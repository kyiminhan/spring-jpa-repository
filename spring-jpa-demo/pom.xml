	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.kyiminhan</groupId>
	<artifactId>spring-jpa-demo</artifactId>
	<version>0.0.1</version>
	<packaging>pom</packaging>
	<modules>
		<module>spring-jpa-demo-001</module>
		<module>spring-jpa-demo-002</module>
		<module>spring-jpa-demo-003</module>
	</modules>
	<properties>
		<failOnMissingWebXml>false</failOnMissingWebXml>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<maven.compiler.target>1.8</maven.compiler.target>
		<maven.compiler.source>1.8</maven.compiler.source>
		<!-- JSP-Servlet version -->
		<failOnMissingWebXml>false</failOnMissingWebXml>
		<jsp-api.version>2.3.1</jsp-api.version>
		<jsp-jstl.version>1.2.1</jsp-jstl.version>
		<jsp-taglibs.version>1.1.2</jsp-taglibs.version>
		<servlet-api.version>3.1.0</servlet-api.version>
		<!-- maven plugin version -->
		<maven-war-plugin.version>3.2.2</maven-war-plugin.version>
		<maven-compiler-plugin.version>3.8.0</maven-compiler-plugin.version>
		<!-- spring framework and spring security version -->
		<spring.version>5.1.5.RELEASE</spring.version>
		<spring.secrity.version>5.1.5.RELEASE</spring.secrity.version>
		<spring-data-jpa.version>2.1.8.RELEASE</spring-data-jpa.version>
		<hibernate.version>5.4.1.Final</hibernate.version>
		<aspectj.version>1.9.2</aspectj.version>
		<!-- lombok logback version -->
		<lombok.version>1.18.6</lombok.version>
		<logback.version>1.2.3</logback.version>
		<slf4j.version>1.7.25</slf4j.version>
		<mysql-connector.version>8.0.28</mysql-connector.version>
		<common-dbcp2.version>2.6.0</common-dbcp2.version>
		<!-- javax mail version -->
		<java.version>1.8</java.version>
		<javax-mail.version>1.6.1</javax-mail.version>
		<!-- Thymeleaf version -->
		<thymeleaf.version>3.0.11.RELEASE</thymeleaf.version>
		<thymeleaf.extras>3.0.3.RELEASE</thymeleaf.extras>
		<!-- Apache Common Version -->
		<apache.common.lang.version>3.8.1</apache.common.lang.version>
		<apache.common.collections.version>4.3</apache.common.collections.version>
		<apache.common.io.version>2.6</apache.common.io.version>
		<apache.common.text.version>1.6</apache.common.text.version>

	</properties>
	<dependencies>
		<!-- SPRING-FRAMEWORK Dependencies -->
		<!-- https://mvnrepository.com/artifact/org.springframework/spring-core -->
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-core</artifactId>
			<version>${spring.version}</version>
		</dependency>
